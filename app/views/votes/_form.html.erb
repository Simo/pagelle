<%= form_for @vote do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :list_id %><br />
    <%= f.text_field :list_id %>
  </p>
  <p>
    <%= f.label :voto %><br />
    <%= f.text_field :voto %>
  </p>
  <p>
    <%= f.label :note %><br />
    <%= f.text_field :note %>
  </p>
  <p>
    <%= f.label :motivo %><br />
    <%= f.text_area :motivo %>
  </p>
  <p><%= f.submit %></p>
<% end %>
